{
  "name": "huijiewei/yii2-star-rating-input",
  "description": "Another plugin that eases the generation of rating stars for jQuery and Bootstrap.",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "yii2-extension",
  "homepage": "https://github.com/huijiewei/yii2-star-rating-input.git",
  "keywords": [
    "yii2",
    "extension",
    "rating-input",
    "assets"
  ],
  "authors": [
    {
      "name": "Huijie Wei",
      "email": "huijiewei@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "yiisoft/yii2": "^2.0",
    "yiisoft/yii2-bootstrap": "^2.0",
    "huijiewei/yii2-fontawesome": "^2.0",
    "npm-asset/bootstrap-rating-input": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "huijiewei\\StarRatingInput\\": "src"
    }
  },
  "repositories": {
    "asset": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  }
}